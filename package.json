{
  "name": "react-boilerplate",
  "version": "1.8.1",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild:pt": "npm run build:clean",
    "prebuildUat:pt": "npm run build:clean",
    "prebuildPro:pt": "npm run build:clean",
    "prebuildTest": "npm run build:clean",
    "prebuildPro": "npm run build:clean",
    "prebuildUat": "npm run build:clean",
    "build": "npm run buildUat",
    "fat": "npm run buildTest",
    "pro": "npm run buildPro",
    "uat": "npm run buildUat",
    "fat:pt": "npm run build:pt",
    "uat:pt": "npm run buildUat:pt",
    "pro:pt": "npm run buildPro:pt",
    "buildTest": "cross-env NODE_ENV=production ENV_TYPE=test ENV_TARGET=portal webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "buildPro": "cross-env NODE_ENV=production ENV_TYPE=prod ENV_TARGET=portal webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "buildUat": "cross-env NODE_ENV=production ENV_TYPE=uat ENV_TARGET=portal webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:clean": "npm run test:clean && rimraf ./dist",
    "build:dll": "cross-env ENV_TARGET=portal node ./internals/scripts/dependencies.js",
    "build:pt": "cross-env NODE_ENV=production ENV_TYPE=test ENV_TARGET=parttime webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "buildUat:pt": "cross-env NODE_ENV=production ENV_TYPE=uat ENV_TARGET=parttime webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "buildPro:pt": "cross-env NODE_ENV=production ENV_TYPE=prod ENV_TARGET=parttime webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "start": "cross-env NODE_ENV=development ENV_TYPE=test ENV_TARGET=portal node server",
    "startTest": "cross-env NODE_ENV=development ENV_TYPE=test ENV_TARGET=portal node server",
    "startPro": "cross-env NODE_ENV=development ENV_TYPE=prod ENV_TARGET=portal node server",
    "startUat": "cross-env NODE_ENV=development ENV_TYPE=uat ENV_TARGET=portal node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:pt": "cross-env NODE_ENV=development ENV_TYPE=test ENV_TARGET=parttime node server",
    "startUat:pt": "cross-env NODE_ENV=development ENV_TYPE=uat ENV_TARGET=parttime node server",
    "startPro:pt": "cross-env NODE_ENV=development ENV_TYPE=prod ENV_TARGET=parttime node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "pretty-quick --staged --pattern app/**/*.js",
    "test:clean": "rimraf ./coverage",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "docz": "docz dev",
    "docz:build": "docz build",
    "buildCourseware": "rimraf ./build-courseware &&cross-env NODE_ENV=production ENV_TYPE=test webpack --config internals/webpack/webpack.prod.babel.courseware.js --color -p --progress"
  },
  "lint-staged": {
    "app/**/*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css",
      "font-awesome",
      "fs",
      "moment"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "@zm-fe/zm-jssdk": "^1.1.6",
    "ali-oss": "^6.1.1",
    "antd": "2.13.2",
    "babel-polyfill": "6.20.0",
    "chalk": "1.1.3",
    "components": "0.1.0",
    "compression": "1.6.2",
    "containers": "0.0.1",
    "echarts": "4.0.4",
    "enhanced-resolve": "3.4.1",
    "express": "4.14.0",
    "font-awesome": "4.7.0",
    "fontfaceobserver": "2.0.7",
    "hash.js": "^1.1.7",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "ip": "1.1.4",
    "jsencrypt": "^3.0.0-rc.1",
    "jspdf": "1.3.5",
    "katex": "0.11.0",
    "lodash": "^4.17.2",
    "minimist": "1.2.0",
    "moment": "2.18.1",
    "plupload": "^2.3.6",
    "qrcode.react": "0.7.2",
    "react": "15.4.1",
    "react-color": "2.13.8",
    "react-contextmenu": "2.9.2",
    "react-cropper": "1.0.0",
    "react-dom": "^15.4.1",
    "react-draggable": "^3.0.5",
    "react-fontawesome": "1.6.1",
    "react-froala-wysiwyg": "2.7.3",
    "react-helmet": "3.2.2",
    "react-input-range": "1.3.0",
    "react-intl": "2.1.5",
    "react-katex": "1.2.0",
    "react-modal": "3.0.4",
    "react-quill": "1.2.7",
    "react-redux": "4.4.6",
    "react-responsive-carousel": "^3.1.41",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.6",
    "react-router-scroll": "0.4.1",
    "react-scroll": "1.6.1",
    "react-select": "1.2.1",
    "react-sortable-hoc": "0.6.8",
    "reactcss": "1.2.3",
    "redux": "3.6.0",
    "redux-immutable": "3.0.8",
    "redux-saga": "0.14.0",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "styled-components": "3.4.10",
    "svgaplayerweb": "2.1.0",
    "svgo": "0.7.2",
    "utils": "0.3.1",
    "wait-promise": "0.4.1",
    "wangeditor": "3.1.1",
    "warning": "3.0.0",
    "whatwg-fetch": "3.0.0",
    "zm-tk-ace": "^0.1.2-beta.2",
    "zm-tr-ace": "^0.1.26-alpha"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-react-inline-elements": "7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "cheerio": "0.22.0",
    "circular-dependency-plugin": "2.0.0",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "coveralls": "2.11.15",
